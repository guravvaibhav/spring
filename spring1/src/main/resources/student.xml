<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="std1" class="com.spring.collection.Student">
		<property name="roll" value="101"></property>
		<property name="name" value="vaibhav"></property>
		<property name="subjects">
			<list>
				<value>java</value>
				<value>database</value>
				<value>microservices</value>
			</list>
		</property>
		<property name="mob">
			<set>
				<value>1234</value>
				<value>5678</value>
				<value>9876</value>
			</set>
		</property>
		<property name="subMarks">
			<map>
				<entry key="java" value="90"></entry>
				<entry key="database" value="36"></entry>
				<entry key="microservices" value="99"></entry>
			</map>
		</property>
	</bean>
</beans>
